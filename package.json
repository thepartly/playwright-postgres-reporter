{
  "name": "playwright-postgres-reporter",
  "version": "0.1.0",
  "description": "Playwright plugin which reports tests and test sessions results and data to Postgresql Database.",
  "license": "MIT",
  "author": "Partly Group",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write .",
	"release": "npm publish --access public"
  },
  "dependencies": {
    "@playwright/test": "1.49.1",
    "knex": "3.1.0"
  },
  "devDependencies": {
    "@types/node": "22.10.10",
    "prettier": "3.4.2",
    "playwright": "1.49.1",
    "typescript": "5.7.3"
  },
  "exports": {
    "import": "./build/index.js",
    "require": "./build/index.js",
    "types": "./build/index.d.ts"
  },
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "keywords": [
    "playwright",
    "reporter",
    "console",
    "testing"
  ],
  "publishConfig": {
    "access": "public"
  }
}
